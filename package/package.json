{
  "name": "konsta",
  "version": "4.0.1",
  "description": "Mobile UI components made with Tailwind CSS",
  "repository": {
    "type": "git",
    "url": "https://github.com/konstaui/konsta.git"
  },
  "keywords": [],
  "author": "Vladimir Kharlampidi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/konstaui/konsta/issues"
  },
  "homepage": "https://konstaui.com",
  "engines": {
    "node": ">= 4.7.0"
  },
  "releaseDate": "September 18, 2024",
  "svelte": "./svelte/konsta-svelte.js",
  "exports": {
    "./package.json": "./package.json",
    "./react": {
      "types": "./react/konsta-react.d.ts",
      "require": "./react/cjs/konsta-react.js",
      "import": "./react/esm/konsta-react.js"
    },
    "./vue": {
      "types": "./vue/konsta-vue.d.ts",
      "require": "./vue/konsta-vue.js",
      "import": "./vue/konsta-vue.js"
    },
    "./svelte": {
      "types": "./svelte/konsta-svelte.d.ts",
      "import": "./svelte/konsta-svelte.js",
      "svelte": "./svelte/konsta-svelte.js"
    },
    "./better-react": {
      "types": "./better-react/types/konsta-better-react.d.ts",
      "import": "./better-react/esm/konsta-better-react.js",
      "require": "./better-react/cjs/konsta-better-react.js"
    },
    "./config": "./config.js",
    "./config.js": "./config.js"
  }
}